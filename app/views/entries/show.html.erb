<% content_for :header do %>
    <% binding.pry %>
  <h1><%= @entry.user_notes %></h1>
<% end %>

<% content_for :vertical_nav do %>
  <ul class="nav flex-column">
    <% if @entry.user.id == current_user.id %>
      <li class="nav-item mb-3">
        <%= button_to "Update Entry", edit_entry_path(@entry), :method => "get", class: "btn btn-info btn-block" %>
      </li>
    <% end %>
  </ul>
<% end %>


